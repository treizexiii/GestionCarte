        </Frame>
        <Frame x:Name="Frame2" Content="" HorizontalAlignment="Left" Height="192" Margin="324,23,0,0" VerticalAlignment="Top" Width="264" Background="{DynamicResource {x:Static SystemColors.AppWorkspaceBrushKey}}" NavigationUIVisibility="Hidden"/>
