        <Frame x:Name="Frame1" Content="" HorizontalAlignment="Left" Height="192" Margin="10,23,0,0" VerticalAlignment="Top" Width="287" Background="{DynamicResource {x:Static SystemColors.AppWorkspaceBrushKey}}" NavigationUIVisibility="Hidden">
